{
  "name": "homepage",
  "scripts": {
    "dev": "npx parcel serve 'src/**/*.html'",
    "clean": "rm -rf ./photon && rm -rf ./dist",
    "build": "parcel build 'src/index.html' 'src/**/*.html' --no-source-maps --no-cache",
    "photon": "photon ./dist"
  },
  "targets": {
    "browser": {
      "includeNodeModules": true,
      "isLibrary": false
    }
  },
  "repository": "https://github.com/Frank-Mayer/homepage.git",
  "author": "Frank Mayer <frank.mayer1907@protonmail.com>",
  "license": "MIT",
  "private": false,
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.3.2",
    "autoprefixer": "^10.4.2",
    "parcel": "^2.3.2",
    "postcss": "^8.4.7",
    "process": "^0.11.10",
    "sass": "^1.49.9",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@frank-mayer/magic": "^1.3.2",
    "photon-re": "^0.1.6"
  }
}
