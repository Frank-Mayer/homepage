  "bugs": {
    "url": "https://github.com/Frank-Mayer/homepage/issues"
  },
  "homepage": "https://github.com/Frank-Mayer/homepage#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "photon-cli": "^0.2.0",
    "sass": "^1.49.7",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@frank-mayer/magic": "^1.3.2",
    "photon-re": "^0.1.6"
  }
}
