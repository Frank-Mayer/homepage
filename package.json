  "license": "MIT",
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "devDependencies": {
    "@frank-mayer/yahp": "^0.0.1",
    "@parcel/transformer-postcss": "2.3.x",
    "@parcel/transformer-sass": "2.3.x",
    "autoprefixer": "^10.4.4",
    "parcel": "2.3.x",
    "photon-cli": "^0.4.0",
    "postcss": "^8.4.12",
    "sass": "^1.50.0",
    "showdown": "^2.1.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@frank-mayer/magic": "^4.0.0",
    "photon-re": "^0.3.2"
  }
