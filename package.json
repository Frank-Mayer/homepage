    "browser": {
      "includeNodeModules": {
        "@frank-mayer/magic": true
      }
    }
  },
  "scripts": {
    "dev": "npx parcel serve 'src/**/*.html'",
    "clean": "rm -rf ./photon && rm -rf ./dist",
    "build": "photon ./src --no-cache"
