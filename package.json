{
  "name": "homepage",
  "scripts": {
    "dev": "parcel serve 'src/**/*.html'",
    "clean": "rm -rf './.parcel-cache' && rm -rf './dist'",
    "pre-b": "node ./html-proc.mjs",
    "build": "parcel build 'src/index.html' 'src/**/*.html' 'src/hotlink-io/*' --no-source-maps --no-cache --public-url 'https://frank-mayer.io/'",
    "post-b": "photon './dist'"
  },
  "repository": "https://github.com/Frank-Mayer/homepage.git",
